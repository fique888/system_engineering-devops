#!/usr/bin/env bash
# This script lists files in the current directory, printing only the part after the first dash (-)

for file in *; do
  # Skip hidden files (should be already excluded by '*', but just in case)
  [[ "$file" == .* ]] && continue

  # Print the part after the first dash if present, else skip
  if [[ "$file" == *-* ]]; then
    echo "${file#*-}"
  fi
done
